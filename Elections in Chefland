#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll a, b, c;
    cin >> a >> b >> c;

    if (a > 50)
        cout << "A\n";
    else if (b > 50)
        cout << "B\n";
    else if (c > 50)
        cout << "C\n";
    else
        cout << "NOTA\n";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}

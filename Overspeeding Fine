#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll s;
    cin >> s;

    if (s <= 70)   
        cout << 0 << nline;
    else if (s > 70 and s <= 100)
        cout << 500 << nline;
    else
        cout << 2000 << nline;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}

#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll n;
    cin >> n;

    for (int i = n / 2; i >= 1; i--)
        cout << i << " ";
    for (int i = n / 2 + 1; i <= n; i++)
        cout << i << " ";
    cout << nline;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}

#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll n;
    cin >> n;

    ll i = 1, j = 2;
    ll maxHeight = 0;
    while (i <= n)
    {
        i += j;
        j++;
        maxHeight++;
    }

    cout << maxHeight << nline;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}

#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll x, y;
    cin >> x >> y;

    if ((2 * x) > (5 * y))
        cout << "Chocolate\n";
    else if ((2 * x) < (5 * y))
        cout << "Candy\n";
    else
        cout << "Either\n";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}

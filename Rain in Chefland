#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll n;
    cin >> n;

    if (n < 3)   
        cout << "LIGHT\n";
    else if (n >= 3 and n < 7)
        cout << "MODERATE\n";
    else
        cout << "HEAVY\n";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}
